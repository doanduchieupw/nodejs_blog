<div class='mt-3'>
    <div class='row'>
    
        {{#each courses}} 
        <div class="col-sm-6 col-lg-3">
            <div class='card mt-1'>
            <img src='{{this.img}}' class='card-img-top' alt='{{this.name}}' />
                <div class='card-body'>
                    <h5 class='card-title'>{{this.name}}</h5>
                    <p class='card-text'>{{this.desc}}</p>
                    <a href='#' class='btn btn-primary'>Buy</a>
                </div>
            </div>
        </div>
        {{/each}}

    </div>
</div> 
